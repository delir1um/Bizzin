# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: Custom deployment URL for referral links
# If not set, the system will auto-detect based on current hostname
# VITE_DEPLOYMENT_URL=https://your-custom-domain.com

# Cloudflare R2 Configuration (for video storage)
VITE_CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
VITE_CLOUDFLARE_R2_ACCESS_KEY_ID=your_access_key_id
VITE_CLOUDFLARE_R2_SECRET_ACCESS_KEY=your_secret_access_key
VITE_CLOUDFLARE_R2_BUCKET_NAME=your_bucket_name
VITE_CLOUDFLARE_R2_PUBLIC_DOMAIN=your_public_domain

# Pre-launch Mode Configuration
# Set to 'true' to show pre-launch marketing page to unauthenticated users
# Set to 'false' or leave empty for full platform development mode
VITE_PRE_LAUNCH_MODE=true